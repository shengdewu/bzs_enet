<?xml version="1.0" encoding="utf-8" ?>
<config>
	<train>
		<batch_size>10</batch_size>
		<eval_batch_size>25</eval_batch_size>
		<num_epoch>300</num_epoch>
		<stage_two_three>1</stage_two_three>
		<repeat_init_block>1</repeat_init_block>
		<skip>False</skip>
	</train>
    <optimize>
		<learning_rate>0.0005</learning_rate>
		<decay_rate>0.1</decay_rate>
		<epsilon>0.00000001</epsilon>
		<num_epochs_before_decay>100</num_epochs_before_decay>
		<l2_weight_decay>0.0002</l2_weight_decay>
    </optimize>
    <mode>
        <mode_path>E:/workspace/bzs/bzs_enet/mode_path/checkout</mode_path>
        <update_mode_freq>100</update_mode_freq>
    </mode>
	<image>
	<!--
	image_path下文件夹名必须是如下名字
	test   测试文件
	testannot 测试标签文件
	train
	trainannot
	val
	valannot
	-->
	    <image_path>E:/workspace/bzs/TensorFlow-ENet/dataset</image_path>
		<result_path>E:/workspace/bzs/bzs_enet/output</result_path>
        <image_suffix>png</image_suffix>
		<class_num>12</class_num>
	</image>
</config>
